alert: TempoFrontendQueueLatency
expr:  histogram_quantile(0.99, sum by(le) (rate(tempo_query_frontend_queue_duration_seconds_bucket[5m]))) > 1
"for": "5m"
labels:
  severity: critical
annotations:
  summary: Tempo frontend queue latency (instance {{ $labels.instance }})
  description: "The {{ $labels.job }} frontend queue is experiencing {{ printf \"%.2f\" $value }}s 99th percentile latency.\n  VALUE = {{ $value }}\n  LABELS = {{ $labels }}"