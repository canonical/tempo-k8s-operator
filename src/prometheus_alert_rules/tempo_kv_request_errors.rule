alert: TempoKVRequestErrors
expr:  100 * sum(rate(tempo_kv_request_duration_seconds_count{status_code=~"5.."}[5m])) by (route) / sum(rate(tempo_kv_request_duration_seconds_count[5m])) by (route)  > 10
"for": "15m"
labels:
  severity: critical
annotations:
  summary: Tempo kv store request errors (instance {{ $labels.instance }})
  description: "The {{ $labels.job }} {{ $labels.route }} KV store requests is experiencing {{ printf \"%.2f\" $value }}% error rate.\n  VALUE = {{ $value }}\n  LABELS = {{ $labels }}"